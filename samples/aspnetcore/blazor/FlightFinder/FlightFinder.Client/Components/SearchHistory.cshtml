<h2>SearchHistory (@SearchHistoryList.Count())</h2>

@foreach (var item in SearchHistoryList)
{
<ul class="list-group mb-4">
    <li class="list-group-item justify-content-between">
        <div>
            <nobr>@item.FromAirport</nobr> -
            <nobr>@item.ToAirport</nobr>
        </div>
    </li>
</ul>
}

@functions
{
    public ICollection<SearchCriteria> SearchHistoryList { get; set; }
}
